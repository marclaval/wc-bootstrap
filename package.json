{
  "name": "wc-bootstrap",
  "version": "0.0.1",
  "description": "Web Components for Bootstrap",
  "main": "dist/collection/index.js",
  "types": "dist/collection/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/wcbs.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "sd concurrent \"stencil build --dev --watch\" \"karma start\" ",
    "ci": "karma start --single-run"
  },
  "dependencies": {
    "@stencil/core": "next",
    "@stencil/router": "latest",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5"
  },
  "devDependencies": {
    "@types/jasmine": "2.6.0",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "jasmine": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-typescript": "3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaval/wc-bootstrap.git"
  },
  "author": "Marc Laval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaval/wc-bootstrap"
  },
  "homepage": "https://github.com/mlaval/wc-bootstrap"
}
